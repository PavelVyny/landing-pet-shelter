
block variables

doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		title #{title}
		<!--[if IE]>
		meta(http-equiv='X-UA-Compatible', content="IE = edge")
		<![endif]-->
		meta(name="Description" content="Shelter for pets in Boston.")
		meta(name="viewport" content="width=device-width,initial-scale=1")
		meta(name="keywords" content="pets, shelter, charity")
		link(rel="preload" as="style" type='text/css' href="static/css/styles.css")
	body
		.wrapper
			include ../modules/header/header
			.content
				include ../modules/intro/intro
				include ../modules/about/about
				include ../modules/pets/pets
				include ../modules/help/help
				include ../modules/donation/donation
			include ../modules/footer/footer
			
		script(src="static/js/vendor/libs.js")
		script(src="static/js/main.js")
		block script
